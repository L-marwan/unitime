<hibernate-mapping package="org.unitime.timetable.model">
    <class name="DistributionObject" table="distribution_object">
        <cache include="non-lazy" usage="read-write"/>
        <id
            name="uniqueId"
            column="uniqueid"
            type="java.lang.Long">
            <generator class="org.unitime.commons.hibernate.id.UniqueIdGenerator">
                <param name="sequence">dist_obj_seq</param>
            </generator>
        </id>

        <many-to-one
            name="distributionPref"
            class="DistributionPref"
            column="dist_pref_id"
            not-null="true"
            insert="true"
            update="true"
            foreign-key="fk_dist_obj_dist_pref"/>

        <many-to-one
            name="prefGroup"
            class="PreferenceGroup"
            column="pref_group_id"
            not-null="true"
            insert="true"
            update="true"
            lazy="false"/>

        <property
            name="sequenceNumber"
            column="sequence_number"
            type="java.lang.Integer"
            not-null="false"
            length="2"/>

    </class>
</hibernate-mapping>
