<hibernate-mapping package="org.unitime.timetable.model">
    <class name="SolverParameter" table="solver_parameter">
        <cache include="non-lazy" usage="read-write"/>
        <id
            name="uniqueId"
            column="uniqueid"
            type="java.lang.Long">
            <generator class="org.unitime.commons.hibernate.id.UniqueIdGenerator">
                <param name="sequence">solver_parameter_seq</param>
            </generator>
        </id>

        <property
            name="value"
            column="value"
            type="java.lang.String"
            not-null="false"
            length="2048"/>

        <many-to-one
            name="definition"
            class="SolverParameterDef"
            column="solver_param_def_id"
            not-null="true"
            foreign-key="fk_solver_param_def"/>

    </class>
</hibernate-mapping>
